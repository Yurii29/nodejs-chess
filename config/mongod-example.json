{
  "systemLog": {
    "destination": "file",
    "path": "/var/log/mongodb/mongod.log",
    "logAppend": true
  },
  "storage": {
    "engine": "mmapv1",
    "dbPath": "/var/lib/mongo",
    "journal": {
      "enabled": true
    }
  },
  "net": {
    "bindIp": "127.0.0.1",
    "port": "27017"
  }
}
